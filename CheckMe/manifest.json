{
    "name": "Chek Me",
    "description": "'Check me' is programm what show your carrent information about you, your device, location and ect.",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16":"assets/computer-icon.png",
        "32":"assets/computer-icon.png",
        "48":"assets/computer-icon.png",
        "128":"assets/computer-icon.png"
    },  
    "action": {
        "default_popup":"menuExtensions/index.html"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "downloads"
    ],
    "background":{
        "service_worker": "/menuExtensions/background.js"
    }
}